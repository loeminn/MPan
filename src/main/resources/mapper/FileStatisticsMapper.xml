<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.loemin.pan.mapper.FileStatisticsMapper">

    <resultMap id="BaseResultMap" type="com.loemin.pan.model.FileStatistics">
            <id property="id" column="id" jdbcType="BIGINT"/>
            <result property="userId" column="user_id" jdbcType="BIGINT"/>
            <result property="fileCount" column="file_count" jdbcType="INTEGER"/>
            <result property="totalCapacity" column="total_capacity" jdbcType="BIGINT"/>
            <result property="usedCapacity" column="used_capacity" jdbcType="BIGINT"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,user_id,file_count,
        total_capacity,used_capacity
    </sql>
</mapper>
